<% if local_assigns[:show] == "tr" %>
  <%= render partial: "software_packages/displays/tr", locals: { software_package: software_package } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "software_packages/displays/li", locals: { software_package: software_package } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "software_packages/displays/div", locals: { software_package: software_package } %>
<% else %>
  <%= render partial: "software_packages/displays/public", locals: { software_package: software_package } %>
<% end %>
