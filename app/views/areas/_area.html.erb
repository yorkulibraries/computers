<% if local_assigns[:show] == "tr" %>
  <%= render partial: "areas/displays/tr", locals: { area: area } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "areas/displays/div", locals: { area: area } %>
<% else %>
  <%= render partial: "areas/displays/public", locals: { area: area } %>
<% end %>
