<% if local_assigns[:show] == "tr" %>
  <%= render partial: "images/displays/tr", locals: { image: image } %>
<% elsif local_assigns[:show] == "li" %>
    <%= render partial: "images/displays/li", locals: { image: image } %>
<% elsif local_assigns[:show] == "div" %>
  <%= render partial: "images/displays/div", locals: { image: image } %>
<% else %>
  <%= render partial: "images/displays/public", locals: { image: image } %>
<% end %>
