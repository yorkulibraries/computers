<p id="notice"><%= notice %></p>

<h1>Summary</h1>
<p><%= @not_in_use.size %> available.</p>
<p><%= @in_use.size %> in-use.</p>

<h2>Not In-use</h2>
<ul>
<% @not_in_use.order('ip').each do |m| %>
  <li><%= m.ip %></li>
<% end %>
</ul>
   
<h2>In-use</h2>
<ul>
<% @in_use.order('last_keep_alive, ip').each do |m| %>
  <li><%= m.ip %>,  <%= time_ago_in_words(m.last_keep_alive) %></li>
<% end %>
</ul>
