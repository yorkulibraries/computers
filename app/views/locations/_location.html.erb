<% if locals[:show] == "tr" %>
  <%= render partial: "locations/displays/tr", locals: { location: location } %>
<% elsif locals[:show] == "public" %>
  <%= render partial: "locations/displays/public", locals: { location: location } %>
<% else %>
  <%= render partial: "locations/displays/div", locals: { location: location } %>
<% end %>
